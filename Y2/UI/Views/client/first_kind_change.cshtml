@model Model.config_file_first_kind

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>first_kind_change</title>
    <script src="~/js/comm/comm.js"></script>
    <link href="~/css/table.css" rel="stylesheet" />
</head>
<body>
    <form name="configfilefirstkindForm" method="post"
          action="/client/Updateconfig_file_first_kind">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--I级机构设置--I级机构变更
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="提交" class="BUTTON_STYLE1" />
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back();">
                </td>
            </tr>
        </table>

        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <input type="hidden" name="ffk_id" value="@Model.ffk_id" />
            <tr>
                <td width="19%" class="TD_STYLE1">
                    I级机构编号
                </td>
                <td width="81%" class="TD_STYLE2">
                    <input type="text" name="first_kind_id" value="@Model.first_kind_id"
                           readonly="readonly" class="INPUT_STYLE1">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    I级机构名称
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="first_kind_name" value="@Model.first_kind_name"
                           readonly="readonly" class="INPUT_STYLE1">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    薪酬发放责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
                </td>
                <td class="TD_STYLE2">
                    <textarea name="first_kind_salary_id" rows="4" class="TEXTAREA_STYLE1">@Model.first_kind_salary_id</textarea>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    销售责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
                </td>
                <td class="TD_STYLE2">
                    <textarea name="first_kind_sale_id" rows="4" class="TEXTAREA_STYLE1">@Model.first_kind_sale_id</textarea>
                </td>
            </tr>
        </table>
    </form>
</body>
</html>
