@model IEnumerable<Model.config_file_first_kind>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>first_kind</title>
    <script src="~/js/comm/comm.js"></script>
    <link href="~/css/table.css" rel="stylesheet" />
</head>
<body>
    <form method="post" action="../customiz/customizAction!findFirstKind">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--I级机构设置
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="button" value="添加" class="BUTTON_STYLE1"
                           onclick="window.location.href = '/client/first_kind_register'" />
                </td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td width="20%" class="TD_STYLE1">
                    I级机构编号
                </td>
                <td width="20%" class="TD_STYLE1">
                    I级机构名称
                </td>
                <td width="25%" class="TD_STYLE1">
                    薪酬发放责任人编号
                </td>
                <td width="25%" class="TD_STYLE1">
                    销售责任人编号
                </td>
                <td width="5%" class="TD_STYLE1">
                    变更
                </td>
                <td width="5%" class="TD_STYLE1">
                    删除
                </td>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td class="TD_STYLE2">
                        @item.first_kind_id
                    </td>
                    <td class="TD_STYLE2">
                        @item.first_kind_name
                    </td>
                    <td class="TD_STYLE2">
                        @item.first_kind_salary_id
                    </td>
                    <td class="TD_STYLE2">
                        @item.first_kind_sale_id
                    </td>
                    <td class="TD_STYLE2">
                        <a href="/client/first_kind_change?id=@item.ffk_id">变更</a>
                    </td>
                    <td class="TD_STYLE2">
                        <a href="/client/DelByffk_id?id=@item.ffk_id" onclick="return confirm('确认要删除吗?')">删除</a>
                    </td>
                </tr>
            }
        </table>
        <p>
    </form>
</body>

</html>
