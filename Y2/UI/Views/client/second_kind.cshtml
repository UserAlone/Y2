@model IEnumerable<Model.config_file_second_kind>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>second_kind</title>
    <script src="~/js/comm/comm.js"></script>
    <link href="~/css/table.css" rel="stylesheet" />
    <script type="text/javascript">

		function submission(){
			document.forms[0].submit();
		}

		function page(currentPage){
			document.getElementById("pid").value=currentPage;
			document.forms[0].submit();
		}
    </script>
</head>
<body>
    <form action="../customiz/customizAction!findSecondKind" method="post">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--II级机构设置
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="button" value="添加" class="BUTTON_STYLE1"
                           onclick="window.location.href = '/client/second_kind_register'" />
                </td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td width="15%" class="TD_STYLE1">
                    I级机构名称
                </td>
                <td width="15%" class="TD_STYLE1">
                    II级机构编号
                </td>
                <td width="15%" class="TD_STYLE1">
                    II级机构名称
                </td>
                <td width="22%" class="TD_STYLE1">
                    薪酬发放责任人编号
                </td>
                <td width="22%" class="TD_STYLE1">
                    销售责任人编号
                </td>
                <td width="5%" class="TD_STYLE1">
                    变更
                </td>
                <td width="5%" class="TD_STYLE1">
                    删除
                </td>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td class="TD_STYLE2">
                        @item.first_kind_name
                    </td>
                    <td class="TD_STYLE2">
                        @item.second_kind_id
                    </td>
                    <td class="TD_STYLE2">
                        @item.second_kind_name
                    </td>
                    <td class="TD_STYLE2">
                        @item.second_salary_id
                    </td>
                    <td class="TD_STYLE2">
                        @item.second_sale_id
                    </td>
                    <td class="TD_STYLE2">
                        <a href="/client/second_kind_change?id=@item.fsk_id">变更</a>
                    </td>
                    <td class="TD_STYLE2">
                        <a href="/client/DelByfsk_id?id=@item.fsk_id" onclick="return confirm('确认要删除吗?')">删除</a>
                    </td>
                </tr>
            }
        </table>
        <br><br>
        <th colspan="7" class="pager"></th>
        <p>
    </form>
</body>
</html>
