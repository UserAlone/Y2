
@{
    ViewBag.Title = "Index";
}

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link href="~/CSS/table.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/jquery-1.7.1.js"></script>
    <script type="text/javascript">
        $(function () {
            $.ajax({
                url: "/config_public_char2/Index2",
                type: "get",
                success: function (dt2) {
                    var dt = JSON.parse(dt2);
                    for (var i = 0; i < dt.length; i++) {
                        var $tr = $("<tr></tr>");
                        var $td = $("<td>" + dt[i]["attribute_kind"] + "</td>");
                        var $td2 = $("<td><a href='/config_public_char2/Delete/" + dt[i]["pbc_id"] + "'>删除</td>")
                        $tr.append($td, $td2);
                        $("#tbales").append($tr);
                    }
                }
            })
        })

    </script>
</head>
<body>
    <form method="post" action="../customiz/customizAction!findAttribute">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--职称设置
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right"></td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1" id="tbales">
            <tr>
                <td width="80%" class="TD_STYLE1">
                    职称名称
                </td>
                <td width="20%" class="TD_STYLE1" align="center">
                    删除
                </td>
            </tr>
        </table>
        <p>

    </form>
</body>
</html>





