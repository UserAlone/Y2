
@{
    Layout = null;
}
@using Model;
<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <link rel="stylesheet"
          href="../../css/cwcalendar.css" type="text/css">
    <script type="text/javascript"
            src="../../javascript/jquery-1.7.2.js">
    </script>
</head>

<body>
    @{ human_file hf = ViewBag.huf_id_list2; }
    <form method="post" action="/humanResources/recovery_list_informationUpdate">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">您正在做的业务是：人力资源--人力资源档案管理--人力资源档案恢复</font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="恢复" class="BUTTON_STYLE1" onclick="return confirm('确定恢复吗?');" />
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back()">
                </td>
            </tr>

        </table>
        <table width="100%" border="1" cellpadding=3 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td class="TD_STYLE1" width="11%">
                    档案编号
                </td>
                <td width="14%" class="TD_STYLE2" colspan="7">
                    <input type="text" id="human_id" name="human_id" value="@hf.human_id"
                           class="INPUT_STYLE2" />
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1" width="11%">
                    I级机构
                </td>
                <td width="14%" class="TD_STYLE2">
                    <input type="hidden" value="@hf.huf_id" name="huf_id" />
                    <input type="hidden" id="first_kind_id" name="first_kind_id" value="@hf.first_kind_id"
                           class="INPUT_STYLE2" />
                    <input type="text" id="first_kind_name" name="first_kind_name" value="@hf.first_kind_name"
                           class="INPUT_STYLE2" />
                </td>
                <td width="11%" class="TD_STYLE1">
                    II级机构
                </td>
                <td width="14%" class="TD_STYLE2">
                    <input type="hidden" id="second_kind_id" name="second_kind_id" value="@hf.second_kind_id"
                           class="INPUT_STYLE2" />
                    <input type="text" id="second_kind_name" name="second_kind_name" value="@hf.second_kind_name"
                           class="INPUT_STYLE2" />
                </td>
                <td width="11%" class="TD_STYLE1">
                    III级机构
                </td>
                <td class="TD_STYLE2" colspan="2">
                    <input type="hidden" id="third_kind_id" name="third_kind_id" value="@hf.third_kind_id "
                           class="INPUT_STYLE2" />
                    <input type="text" id="third_kind_name" name="third_kind_name" value="@hf.third_kind_name"
                           class="INPUT_STYLE2" />
                </td>
                <td rowspan="5" width="13%" style="text-align: center;">
                    <input type="hidden" id="human_picture" name="human_picture" value="@hf.human_picture "
                           class="INPUT_STYLE2" />
                    <img src="~/images/@hf.human_picture" style="width:120px;height:150px;" />
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    职位分类
                </td>
                <td class="TD_STYLE2">
                    <input type="hidden" id="human_major_kind_id" name="human_major_kind_id" value="@hf.human_major_kind_id"
                           class="INPUT_STYLE2" />
                    <input type="text" id="human_major_kind_name" name="human_major_kind_name" value="@hf.human_major_kind_name"
                           class="INPUT_STYLE2" />
                </td>
                <td class="TD_STYLE1">
                    职位名称
                </td>
                <td class="TD_STYLE2">
                    <input type="hidden" id="human_major_id" name="human_major_id" value="@hf.human_major_id"
                           class="INPUT_STYLE2" />
                    <input type="text" id="hunma_major_name" name="hunma_major_name" value="@hf.hunma_major_name"
                           class="INPUT_STYLE2" />
                </td>
                <td class="TD_STYLE1">
                    职称
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" id="human_pro_designation" name="human_pro_designation" value="@hf.human_pro_designation"
                           class="INPUT_STYLE2" />
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    姓名
                </td>
                <td class="TD_STYLE2">
                    <input type="text" id="humanName" name="human_name" value="@hf.human_name"
                           class="INPUT_STYLE2" />
                </td>
                <td class="TD_STYLE1">
                    性别
                </td>
                <td class="TD_STYLE2">
                    <input type="text" id="humanName" name="human_name" value="@hf.human_name"
                           class="INPUT_STYLE2" />
                </td>
                <td class="TD_STYLE1">
                    EMAIL
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" name="human_sex" id="human_sex" value="@hf.human_sex"
                           class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    电话
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_telephone" id="humanTelephone" value="@hf.human_telephone"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    QQ
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_qq" id="humanQq" value="601058497"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    手机
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" name="human_mobilephone" id="humanMobilephone" value="@hf.human_mobilephone"
                           class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    住址
                </td>
                <td colspan="3" class="TD_STYLE2">
                    <input type="text" name="human_address" value="@hf.human_address"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    邮编
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" name="human_postcode" value="@hf.human_postcode"
                           class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    国籍
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_nationality" value="@hf.human_nationality"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    出生地
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_birthplace" value="@hf.human_birthplace"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    生日
                </td>
                <td width="13%" class="TD_STYLE2">
                    <input type="text" name="human_birthday" readonly="readonly" value="@hf.human_birthday"
                           class="INPUT_STYLE2" id="birthday">
                </td>
                <td width="11%" class="TD_STYLE1">
                    民族
                </td>
                <td class="TD_STYLE2" width="14%">
                    <input type="text" name="human_race" readonly="readonly" value="@hf.human_race"
                           class="INPUT_STYLE2" id="birthday">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    宗教信仰
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_religion" readonly="readonly" value="@hf.human_religion"
                           class="INPUT_STYLE2" id="birthday">
                </td>
                <td class="TD_STYLE1">
                    政治面貌
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_party" readonly="readonly" value="@hf.human_party"
                           class="INPUT_STYLE2" id="birthday">
                </td>
                <td class="TD_STYLE1">
                    身份证号码
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_id_card" id="humanIdCard" value="@hf.human_id_card"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    社会保障号码
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_society_security_id" value="110"
                           class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    年龄
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_age" id="human_age" value="@hf.human_age"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    学历
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_educated_degree" id="human_educated_degree" value="@hf.human_educated_degree"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    教育年限
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_educated_years" id="human_educated_years " value="@hf.human_educated_years"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    学历专业
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_educated_major" id="human_educated_major" value="@hf.human_educated_major"
                           class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    薪酬标准
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="salary_standard_name" id="salary_standard_name" value="@hf.salary_standard_name"
                           class="INPUT_STYLE2">
                    <input type="hidden" name="salary_standard_id" id="salary_standard_id" value="@hf.salary_standard_id"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    开户行
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_bank" value="@hf.human_bank"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    帐号
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_account" value="@hf.human_account"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    复核人
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="checker" value="@hf.checker"
                           class="INPUT_STYLE2">
                    <input type="hidden" name="register" value="@hf.register"
                           readonly="readonly" class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    复核时间
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="check_time" value="@hf.check_time"
                           id="nowTime" readonly="readonly"
                           class="INPUT_STYLE2">
                    <input type="hidden" name="regist_time" value="@hf.regist_time"
                           id="nowTime" readonly="readonly"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    特长
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_speciality" value="@hf.human_speciality"
                           id="nowTime" readonly="readonly"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    爱好
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_hobby" value="@hf.human_hobby"
                           id="nowTime" readonly="readonly"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    &nbsp;
                </td>
                <td class="TD_STYLE2">
                    &nbsp;
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    附件名称
                </td>
                <td width="14%" class="TD_STYLE2" colspan="7">
                    <input type="text" name="attachment_name" value="@hf.attachment_name"
                           id="nowTime" readonly="readonly"
                           class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    个人履历
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <textarea name="humanFile.humanHistroyRecords" rows="4"
                              class="TEXTAREA_STYLE1">个人履历</textarea>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    家庭关系信息
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <textarea name="human_family_membership" rows="4"
                              class="TEXTAREA_STYLE1">@hf.human_family_membership</textarea>
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        备注
                    </td>
                    <td colspan="7" class="TD_STYLE2">
                        <textarea name="remark" rows="4" class="TEXTAREA_STYLE1">@hf.remark</textarea>
                    </td>
                </tr>
            </table>
        </form>
    </body>
</html>
