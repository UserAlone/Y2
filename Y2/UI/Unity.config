<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection,Unity.Configuration, Version=5.2.5.0, Culture=neutral, PublicKeyToken=6d32ff45e0ccc69f"/>
  </configSections>
  <unity>
    <containers>
      <!--用户管理-->
      <container name="containerOne">
        <register type="IDAO.RoleUsIDAO,IDAO" mapTo="BAL.RoleUsBAL,BAL" name="RoleUsBAL"></register>       
      </container>
      <container name="containerTwo">
        <register type="IBLL.RoleUsIBLL,IBLL" mapTo="BLL.RoleUsBLL,BLL" name="RoleUsBLL"></register>      
      </container>
      <!--角色管理-->
      <container name="JiaoOne">
        <register type="IDAO.RoleRigIDAO,IDAO" mapTo="BAL.RoleRigBAL,BAL" name="RoleRigBAL"></register>
      </container>
      <container name="JiaoTwo">
        <register type="IBLL.RoleRigIBLL,IBLL" mapTo="BLL.RoleRigBLL,BLL" name="RoleRigBLL"></register>
      </container>

      <!--登录管理-->
      <container name="DlOne">
        <register type="IDAO.UserIDAO,IDAO" mapTo="BAL.UserBAL,BAL" name="UserBAL"></register>
      </container>
      <container name="DlTow">
        <register type="IBLL.UserIBLL,IBLL" mapTo="BLL.UserBLL,BLL" name="UserBLL"></register>
      </container>

      <!--调动管理-->
      <container name="DdOne">
        <register type="IDAO.human_fileIDAO,IDAO" mapTo="BAL.human_fileBAL,BAL" name="human_fileBAL"></register>
      </container>
      <container name="DdTow">
        <register type="IBLL.human_fileIBLL,IBLL" mapTo="BLL.human_fileBLL,BLL" name="human_fileBLL"></register>
      </container>

      <!--调动管理-->
      <container name="DDOne">
        <register type="IDAO.major_changeIDAO,IDAO" mapTo="BAL.major_changeBAL,BAL" name="major_changeBAL"></register>
      </container>
      <container name="DDTow">
        <register type="IBLL.major_changeIBLL,IBLL" mapTo="BLL.major_changeBLL,BLL" name="major_changeBLL"></register>
      </container>

    </containers>
  </unity>
</configuration>